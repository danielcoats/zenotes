{
  "manifest_version": 2,
  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "browser_action": {
    "default_title": "Click here!"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
